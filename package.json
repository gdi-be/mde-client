    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/bun": "^1.2.0",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "svelte": "^5.19.5",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.4",
    "typescript": "^5.6.3",
