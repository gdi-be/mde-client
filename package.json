    "@types/proj4": "^2.5.6",
    "cookie": "^1.0.1",
    "eventsource": "^3.0.6",
    "jsonwebtoken": "^9.0.2",
    "loggisch": "^0.0.1",
    "marked": "^15.0.2",
    "proj4": "^2.15.0",
    "smui-theme": "^8.0.0-beta.3",
    "yaml": "^2.7.0"
